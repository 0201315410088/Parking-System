@model IEnumerable<testsubject.Models.ImageModel>
@{
    ViewData["Title"] = "My Bookings";
    Layout = "_Layout";
}
<h2>Image List</h2>

<table class="table">
    <thead>
        <tr>
            <th>Image</th>
            <th>Name</th>
            <th>Email</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var image in Model)
        {
            <tr>
                <td>
                    <img src="data:image;base64,@Convert.ToBase64String(image.ImageData)" alt="@image.ImageName" width="100" />
                </td>
                <td>@image.ImageName</td>
                <td>@image.UserEmail</td>
            </tr>
        }
    </tbody>
</table>
